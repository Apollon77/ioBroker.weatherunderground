{
    "common": {
        "name":                     "weatherunderground",
        "version":                  "0.0.1",
        "title":                    "weatherunderground forecast",
        "desc":                     "load weatherunderground weather forecast for your location",
        "platform":                 "Javascript/Node.js",
        "mode":                     "schedule",
        "schedule":                 "*/15 * * * *",
        "icon":                     "wu.png",
        "extIcon":                  "https://raw.githubusercontent.com/dschaedl/ioBroker.weatherunderground/master/admin/wu.png",
        "readme":                   "https://github.com/ioBroker/dschaedl/ioBroker.weatherunderground/blob/master/README.md",
        "license":                  "MIT",
        "npmLibs": [],
        "type":                     "weather",
        "keywords":                 ["iobroker", "weather", "forecast", "wu", "weatherunderground"],
        "loglevel":                 "info",
        "enabled":                  true
    },
    "native": {
        "apikey": "getYourOwn",
        "location": "HKG"
    },
    "objects": [
    ],
    "instanceObjects": [
        {
            "_id":  "temp",
            "type": "state",
            "common": {
                "name": "Temperature",
                "type": "number",
                "role": "value.temperature",
                "unit": "°C",
                "read": true,
                "write": false
            },
            "native": {}
        }

    ]

}